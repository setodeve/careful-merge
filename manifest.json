{
  "manifest_version": 3,
  "name": "Careful Merge",
  "version": "1.0.0",
  "description": "GitHubのプルリクエストでマージ方法を確認するダイアログを表示します",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
